# Add ccache to PATH
export PATH=/usr/lib/ccache:$PATH
export VLR_ROOT=/home/daniel/


# Override roscompile for threading
function zoox-compile(){
(
  cd $ROS_WORKSPACE
  pwd
  catkin_make -j -l8
)
}


# Override roscompile for threading
function zoox-compile-tests(){
(
  cd $ROS_WORKSPACE
  pwd
  catkin_make tests -j -l8
)
}


# pull down tests
function zoox-download-tests(){
(
  cd $ROS_WORKSPACE
  pwd
  catkin_make download_test_data
)
}

